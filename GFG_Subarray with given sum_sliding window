#include <iostream>
using namespace std;


int fun(int arr[],int n,int sum,int s,int low,int high)
{
    if(high<n)
    {
        if(s<sum)
        {
            high++;
            s=s+arr[high];
            fun(arr,n,sum,s,low,high);
        }
        else if(s>sum)
        {
            s = s-arr[low];
            low++;
            fun(arr,n,sum,s,low,high);
        }
        else if(s==sum)
        {
            cout<<low+1<<" "<<high+1;
            return 0;
        }
    }
    else
    return -1;
}

int main() {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int n,sum;
	    cin>>n>>sum;
	    int arr[n];
	    for(int i=0;i<n;i++)
	    {
	        cin>>arr[i];
	    }
	    int s=arr[0];
	    fun(arr,n,sum,s,0,0);
	    cout<<endl;
	}
	return 0;
}
